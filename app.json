{
  "expo": {
    "name": "FonyBox",
    "slug": "fonybox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "FonyBox",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stacknyu.fonybox"
    },
    "android": {
      "package": "com.stacknyu.fonybox",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/FONYBOX.png",
        "backgroundColor": "#F52936"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/FONYBOX.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F52936"
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "19207ad0-393f-437c-9e67-e0547afc9361"
      }
    }
  }
}
